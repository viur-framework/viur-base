{
    "default": {
        "admin": "4.0.24",
        "builds": {
            "admin": {
                "command": "viur install admin 4.0.24",
                "kind": "exec"
            },
            "npm": {
                "command": "build",
                "kind": "npm",
                "source": ""
            }
        },
        "core": "3.5.12",
        "distribution_folder": "./deploy",
        "format": "1.1.1",
        "sources_folder": "./sources",
        "vi": "not-in-use"
    },
    "develop": {
        "application_name": "{{app_id}}",
        "version": "dev-$(user)"
    },
    "live": {
        "application_name": "{{app_id}}",
        "version": "live-$(year)-$(month)-$(day)-$(hour)-$(minute)-$(second)"
    }
}